{
  "capacityProviderStrategy": [
    {
      "base": 0,
      "capacityProvider": "FARGATE_SPOT",
      "weight": 1
    }
  ],
  "deploymentConfiguration": {
    "deploymentCircuitBreaker": {
      "enable": false,
      "rollback": false
    },
    "maximumPercent": 200,
    "minimumHealthyPercent": 100
  },
  "desiredCount": 1,
  "enableECSManagedTags": false,
  "enableExecuteCommand": true,
  "healthCheckGracePeriodSeconds": 60,
  "loadBalancers": [
    {
      "containerName": "nginx",
      "containerPort": 80,
      "targetGroupArn": "{{ tfstate `data.terraform_remote_state.routing_appsnsapp_link.outputs.lb_target_group_snsapp_arn` }}"
    }
  ],
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "assignPublicIp": "DISABLED",
      "securityGroups": [
        "{{ tfstate `data.terraform_remote_state.network_main.outputs.security_group_vpc_id` }}",
        "{{ tfstate `data.terraform_remote_state.network_main.outputs.security_group_db_snsapp_id` }}",
        "{{ tfstate `data.terraform_remote_state.network_main.outputs.security_group_cache_snsapp_id` }}"
      ],
      "subnets": [
        "{{ tfstate `data.terraform_remote_state.network_main.outputs.subnet_private[\"a\"].id` }}",
        "{{ tfstate `data.terraform_remote_state.network_main.outputs.subnet_private[\"c\"].id` }}"
      ]
    }
  },
  "placementConstraints": [],
  "placementStrategy": [],
  "platformVersion": "1.4.0",
  "schedulingStrategy": "REPLICA",
  "serviceRegistries": [],
  "tags": [
    {
      "key": "Env",
      "value": "{{ must_env `ENV_NAME` }}"
    },
    {
      "key": "System",
      "value": "{{ must_env `SYSTEM_NAME` }}"
    },
    {
      "key": "Name",
      "value": "{{ must_env `SYSTEM_NAME` }}-{{ must_env `ENV_NAME` }}-{{ must_env `SERVICE_NAME` }}"
    }
  ]
}
